<h1><%= @lesson.title %></h1>

<h2>Lesson Content</h2>
<ul>
<% @combinedResourceAssessments.each do |obj| %>
  <% if obj.class == Resource %>
  <li>Resource: <%= obj.content %>, Complete? <%= obj.completed %></li>
  <% else %>
  <li>Assessment: challenge? <%= obj.challenge? %>, Complete? <%= obj.completed %></li>
  <% end %>
<% end %>
</ul>
